{
    "manifest_version": 2,
  
    "name": "Call meteo",
    "description": "Geolocalise you and give the current outside temperature",
    "version": "0.0.0.2",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "permissions": ["geolocation"]
}